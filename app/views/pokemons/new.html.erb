<%= form_for(@pokemon) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description%>
  <%= f.text_field :description %>
  <%= f.label :element %>
  <%= f.text_field :element %>
  <%= f.label :evolution_level %>
  <%= f.text_field :evolution_level %>
  <%= f.submit %>
<% end %>